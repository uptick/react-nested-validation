{
  "name": "react-nested-validation",
  "version": "0.0.2",
  "description": "A toolkit for performing nested validation of React forms.",
  "esnext": "src/index.js",
  "main": "dist/index.js",
  "scripts": {
    "build": "webpack --mode development --progress --colors",
    "watch": "webpack --mode development --progress --colors --watch",
    "profile": "webpack --mode development --profile --json > stats.json",
    "build:prod": "webpack --mode production --progress --colors --env.production=true",
    "profile:prod": "webpack --mode production --profile --json > stats.json",
    "analyze": "webpack-bundle-analyzer stats.json",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -o var/storybook -c .storybook",
    "lint": "eslint src",
    "test": "mocha --require babel-polyfill --require babel-core/register -R nyan --recursive tests/*.js",
    "test:one": "mocha --require babel-polyfill --require babel-core/register -R nyan",
    "demo:first": "git subtree push --prefix demo-site origin gh-pages",
    "demo:push": "git push origin `git subtree split --prefix demo-site master`:gh-pages --force"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uptick/react-nested-validation"
  },
  "author": "Uptick Pty. Ltd.",
  "license": "MIT",
  "homepage": "https://github.com/uptick/react-nested-validation",
  "devDependencies": {
    "@storybook/addon-info": "^3.2.17",
    "@storybook/addon-storyshots": "3.3.0-alpha.6",
    "@storybook/react": "3.3.0-alpha.6",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "code": "^5.2.0",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "jsdom": "^11.10.0",
    "mocha": "^5.1.1",
    "peer-deps-externals-webpack-plugin": "^1.0.2",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-hot-loader": "^4.0.1",
    "webpack": "^4.5.0",
    "webpack-bundle-analyzer": "^2.11.2",
    "webpack-cli": "^2.0.14"
  },
  "peerDependencies": {
    "react": "^15 - ^16.3.1",
    "react-dom": "^15 - ^16.3.1"
  }
}
